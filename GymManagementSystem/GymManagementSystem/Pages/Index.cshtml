@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/1.jpg" alt="Dance Training" class="img-responsive" style="height:200px; width:1200px" />
        </div>
        <div class="item">
            <img src="~/images/2.jpg" alt="Studio Excerices" class="img-responsive" style="height:200px; width:1200px" />            
        </div>
        <div class="item">
            <img src="~/images/3.jpg" alt="Weight Training" class="img-responsive" style="height:200px; width:1200px" />            
        </div>
    </div>
</div>